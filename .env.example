# Required Secrets (generate secure random strings for these)
JWT_SECRET=your-secure-jwt-secret-here
SESSION_SECRET=your-secure-session-secret-here
PORT=3000

# Default Admin User (created on first startup if no users exist)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change-this-password

# OIDC Configuration (optional - for SSO login)
OIDC_ISSUER=
OIDC_AUTH_URL=
OIDC_TOKEN_URL=
OIDC_USERINFO_URL=
OIDC_CLIENT_ID=
OIDC_CLIENT_SECRET=
OIDC_CALLBACK_URL=http://localhost:3000/auth/callback

# Frontend URL (used for OIDC redirects)
FRONTEND_URL=http://localhost:3000

# OpenRouter API Key (required for AI analysis features)
# Get your key at https://openrouter.ai/keys
OPENROUTER_API_KEY=
